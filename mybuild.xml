<?xml version="1.0"?>
<project name="GridWorld" default="init" basedir=".">
    <target name="init">
      <mkdir dir="classes"/>
      <copy todir="classes">
         <fileset dir="framework">
            <include name="**/*.gif"/>
            <include name="**/*.properties"/>
            <include name="**/*.html"/>
         </fileset>
         <fileset dir="project">
            <include name="**/*.gif"/>
            <include name="**/*.properties"/>
            <include name="**/*.html"/>
         </fileset>
      </copy>
   </target>
</project>